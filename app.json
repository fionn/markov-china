    "keywords": ["twitter", "twitter-bot", "china", "headlines", "ebooks"],
    "repository": "https://github.com/fionn/markov-china",
    "addons": ["scheduler"],
    "env": {
        "API_KEY": {
            "description": "Twitter API key"
